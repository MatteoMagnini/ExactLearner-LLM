    <!-- http://www.geneontology.org/go#TRANS_0000017 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000017">
        <rdfs:label>not curated</rdfs:label>
        <go:is_obsolete>true</go:is_obsolete>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000018 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000018">
        <rdfs:label>placental</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000006"/>
        <go:def>&quot;Placental transmission is a congenital transmission process during which the pathogen is transmitted directly from mother to child across the placental barrier.&quot; [REF:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, URL:http\://www.nlm.nih.gov/medlineplus/mplusdictionary.html]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000019 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000019">
        <rdfs:label>arthropod borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000005"/>
        <go:def>&quot;Arthropod borne transmission is a vector-borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an animate intermediary arthropod vector to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, URL:http\://www2a.cdc.gov/phtn/catalog/pdf-file/Epi_Course.pdf]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000020 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000020">
        <rdfs:label>insect borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000019"/>
        <go:def>&quot;Insect borne transmission is an arthropod vector-borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary insect vector such as a flea or mosquito to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, URL:http\://www.cals.ncsu.edu/course/ent425/text18/humanvectors.html]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000021 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000021">
        <rdfs:label>mosquito borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000020"/>
        <go:def>&quot;Mosquito borne transmission is an insect borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary mosquito vector to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, wiki:http\://en.wikipedia.org/wiki/Mosquito]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000022 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000022">
        <rdfs:label>Anopheles gambiae borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000021"/>
        <go:def>&quot;Anopheles gambiae borne transmission is a mosquito borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary mosquito vector of the species Anopheles gambiae to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, wiki:http\://en.wikipedia.org/wiki/Mosquito]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000023 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000023">
        <rdfs:label>flea borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000020"/>
        <go:def>&quot;Flea borne transmission is an insect borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary flea vector to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, wiki:http\://en.wikipedia.org/wiki/Flea]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000024 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000024">
        <rdfs:label>tick borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000027"/>
        <go:def>&quot;Tick borne transmission is an arachnid borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary tick vector to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, wiki:http\://en.wikipedia.org/wiki/Tick, wiki:http\://en.wikipedia.org/wiki/Vector_%28disease%29]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000025 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000025">
        <rdfs:label>copepod borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000015"/>
        <go:def>&quot;Copepod borne transmission is a vector-borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an animate intermediary copepod vector to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, URL:http\://www2a.cdc.gov/phtn/catalog/pdf-file/Epi_Course.pdf]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000026 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000026">
        <rdfs:label>Dracunculus medinensis borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000025"/>
        <go:exact_synonym>&quot;Guinea worm borne&quot; []</go:exact_synonym>
        <go:def>&quot;Dracunculus medinensis borne transmission is a copepod borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an animate intermediary copepod vector of the species Dracunculus medinensis to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, URL:http\://www2a.cdc.gov/phtn/catalog/pdf-file/Epi_Course.pdf]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#TRANS_0000027 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#TRANS_0000027">
        <rdfs:label>arachnid borne</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/go#TRANS_0000019"/>
        <go:def>&quot;Arachnid borne transmission is an arthropod vector-borne transmission process during which the pathogen is indirectly transferred from a reservoir, source or host via an intermediary arthropod vector such as a tick to another host.&quot; [Gemina:lschriml &quot;Lynn Schriml &lt;lschriml@som.umaryland.edu&gt;&quot;, wiki:http\://en.wikipedia.org/wiki/Vector_%28disease%29]</go:def>
