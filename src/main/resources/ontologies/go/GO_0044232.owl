    <!-- http://purl.obolibrary.org/obo/GO_0044232 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0044232">
        <rdfs:label>organelle membrane contact site</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0110165"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
