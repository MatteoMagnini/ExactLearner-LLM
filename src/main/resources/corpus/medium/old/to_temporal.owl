        <go:namespace>plant_growth_and_development_stage</go:namespace>
        <go:synonym>&quot;pollen stage 7 in Arabidopsis&quot; []</go:synonym>
        <go:def>&quot;Stage of microgametophyte development during which a large vacuole is formed in each microspore causing a rapid increase in size and a displacement of the nucleus to one side.&quot; [TAIR:ki]</go:def>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#PO_0001014 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#PO_0001014">
        <rdfs:label>I first mitotic division stage</rdfs:label>
