        <go:namespace>plant_growth_and_development_stage</go:namespace>
        <go:def>&quot;Stage of microgametophyte development during which the microspores round up and small vacuoles appear in their cytoplasm.  The microspore walls thicken due to the formation of the exine.&quot; [TAIR:ki]</go:def>
        <go:synonym>&quot;pollen stage 6 in Arabidopsis&quot; []</go:synonym>
    </owl:Class>
    


    <!-- http://www.geneontology.org/go#PO_0001013 -->

    <owl:Class rdf:about="http://www.geneontology.org/go#PO_0001013">
        <rdfs:label>H late unicellular microspore stage</rdfs:label>
